<table class="table table-hover table-condensed">
    <thead>
    <tr>
        <th>score</th>
        <th>T1</th>
        <th>T2</th>
        <th>actions</th>
    </tr>
    </thead>
    <tbody>
    {{#each matches}}
        <tr>
            <td>{{formatNumber interesting minimumFractionDigits="2" maximumFractionDigits="2"}}</td>
            <td>{{t1_originaltext}} <a href="http://twitter.com/{{t1_username}}/status/{{t1_statusid}}">link</a></td>
            <td>{{t2_originaltext}} <a href="http://twitter.com/{{t2_username}}/status/{{t2_statusid}}">link</a></td>
            <td>
                <form action='unrejectmanually/{{id}}' method="post">
                    <button type="submit" class="btn btn-primary">Unreject</button>
                </form>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>