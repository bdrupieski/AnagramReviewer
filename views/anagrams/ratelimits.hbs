<h3>Rate Limits</h3>
<div class="row">
    <div class="col-md-6">
        <table class="table table-hover table-condensed">
            <tr>
                <th>Call</th>
                <th>Limit</th>
                <th>Used</th>
                <th>Remaining</th>
                <th>Reset</th>
            </tr>
            {{#each limitsFormatted}}
                <tr>
                    <td>{{call}}</td>
                    <td>{{limit}}</td>
                    <td>{{used}}</td>
                    <td>{{remaining}}</td>
                    <td>{{reset}}</td>
                </tr>
            {{/each}}
        </table>
    </div>
</div>

<h3>Raw Rate Limits</h3>
<pre>{{rateLimits}}</pre>