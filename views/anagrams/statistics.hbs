        function buildChartInterestingFactorDistributionForDay(scoreDistributionStats, cssSelector) {
            const data = [{
                values: scoreDistributionStats.total,
                key: 'Total',
                disabled: true,
            }, {
                values: scoreDistributionStats.unreviewed,
                key: "Unreviewed",
                disabled: true,
            }, {
                values: scoreDistributionStats.reviewed,
                key: 'Reviewed',
            }, {
                values: scoreDistributionStats.rejected,
                key: 'Rejected',
            }];

            nv.addGraph(() => {
                const chart = nv.models.multiBarChart()
                        .options({
                            duration: 300,
                        })
                        .margin({right: 50})
                        .color(d3.scale.category10().range());

                chart.xAxis.axisLabel("Interesting Factor").tickFormat(d3.format('.2f'));
                chart.yAxis.axisLabel("Count").tickFormat(d3.format('i'));
                d3.select(cssSelector).datum(data).call(chart);
                nv.utils.windowResize(chart.update);
                window.dispatchEvent(new Event('resize'));
                return chart;
            });
        }

        function buildChartScoreSurplusByBucket(scoreSurplusByBucket, cssSelector) {
            const data = [{
                values: scoreSurplusByBucket.lcsApprovedSurplus,
                key: 'longest common substring',
            }, {
                values: scoreSurplusByBucket.wcApprovedSurplus,
                key: "word count",
            }, {
                values: scoreSurplusByBucket.edApprovedSurplus,
                key: 'edit distance',
            }, {
                values: scoreSurplusByBucket.ewcApprovedSurplus,
                key: 'English words',
            }, {
                values: scoreSurplusByBucket.tlApprovedSurplus,
                key: 'total length',
            }];

            nv.addGraph(() => {
                const chart = nv.models.lineChart()
                        .options({
                            useInteractiveGuideline : true,
                            duration: 300,
                        })
                        .color(d3.scale.category10().range());

                chart.xAxis.axisLabel("Interesting Factor")
                        .tickFormat(d3.format('.2f'));
                chart.yAxis.axisLabel('Score Surplus')
                        .tickFormat((d) => {
                            if (d == null) {
                                return 'N/A';
                            }
                            return d3.format('.2%')(d);
                        });

                d3.select(cssSelector).datum(data).call(chart);
                nv.utils.windowResize(chart.update);
                return chart;
            });
        }

        const $jsCollapseButtons = $(".js-collapse-button");
        const heavyMinus = "\u2796";
        const heavyPlus = "\u2795";
        $jsCollapseButtons.click(function() {
            $(this).text(function (i, previous) {
                return previous == heavyPlus ? heavyMinus : heavyPlus;
            });
        });

        const numDays = `{{stats.numberOfPastDays}}`;
        if (Number(numDays) > 30) {
            $("#retweets-and-tumblr-posts-by-day-table").removeClass("in");
            $("#stats-by-date-created-table").removeClass("in");
            $jsCollapseButtons.text(heavyPlus);
        } else {
            $jsCollapseButtons.text(heavyMinus);
        }

        function renderTable($template, $target, data) {
            const template = Handlebars.compile($template.html());
            const html = template({rows: data});
            $target.html(html);
        }

        HandlebarsIntl.registerWith(Handlebars);

        const byDayMatchPosted = buildStatsByDateMatchPosted();
        buildChartRetweetsAndTumblrPostsByDay(byDayMatchPosted.series, '#retweets-and-tumblr-posts-by-day-chart');

        const byDayMatchCreated = buildStatsByDateMatchCreated();
        buildChartRetweetedAndUnreviewedByDateMatchCreated(byDayMatchCreated.series, '#percent-reviewed-unreviewed-by-date-created-chart');
        buildChartDateMatchCreatedPercentApprovalFailure(byDayMatchCreated.series, '#percent-approval-failure-by-date-created-chart');
        buildChartDateMatchCreatedCounts(byDayMatchCreated.series, '#counts-by-date-created-chart');
        buildChartDayMatchCreatedAverageInterestingFactor(byDayMatchCreated.series, '#average-interesting-factor-by-action-taken-chart');

        const byScoreBucket = buildStatsByScoreBucket();
        buildChartScoreBucketRetweetedAndUnreviewed(byScoreBucket.series, '#reviewed-unreviewed-by-score-bucket-chart');
        buildChartScoreBucketCounts(byScoreBucket.series, '#totals-by-score-bucket-chart');

        const byTimeOfDayMatchCreated = buildStatsByTimeOfDayMatchCreated();
        buildChartTimeOfDayMatchCreatedCounts(byTimeOfDayMatchCreated.series, '#totals-by-time-of-day-match-created-chart');
        buildChartTimeOfDayMatchCreatedAverageInterestingFactor(byTimeOfDayMatchCreated.series, '#average-interesting-factor-by-time-of-day-match-created-chart');

        const scoreSurplusByBucket = buildScoreSurplusByBucket();
        buildChartScoreSurplusByBucket(scoreSurplusByBucket.series, '#score-surplus-by-bucket-chart');

        renderTable($("#retweets-and-tumblr-posts-by-day-table-template"),
                    $("#retweets-and-tumblr-posts-by-day-table"), byDayMatchPosted.stats);

        renderTable($("#stats-by-date-created-table-template"),
                    $("#stats-by-date-created-table"), byDayMatchCreated.stats);

        renderTable($("#stats-by-score-table-template"),
                    $("#stats-by-score-table"), byScoreBucket.stats);

        renderTable($("#stats-by-time-of-day-match-created-table-template"),
                    $("#stats-by-time-of-day-match-created-table"), byTimeOfDayMatchCreated.stats);

        renderTable($("#score-surplus-by-interesting-factor-table-template"),
                    $("#score-surplus-by-interesting-factor-table"), scoreSurplusByBucket.stats);

        $(".js-sticky-header").stickyTableHeaders();

        $("#interesting-factors-for-day-chart-modal").on('shown.bs.modal', function () {
            // trigger nvd3 to redraw chart once it's visible
            window.dispatchEvent(new Event('resize'));
        });

        $("[data-show-interesting-factor-distrubution]").click(e => {
            $.blockUI({message: '<h4>Loading...</h4>'});
            const dateString = $(e.target).data("match-created");
            const encodedDateString = encodeURIComponent(dateString);
            $.getJSON(`/anagrams/interestingfactorsforday/${encodedDateString}`).done(data => {
                if (data.error) {
                    $.growl.error({message: data.error});
                } else if (data.interestingFactors) {
                    const scores = data.interestingFactors.map(x => new InterestingFactorsForDay(x));
                    const series = new InterestingFactorsForDaySeriesCollection(scores);
                    buildChartInterestingFactorDistributionForDay(series, '#interesting-factors-for-day-chart');

                    $("#interesting-factors-for-day-chart-modal-title").text(data.date);
                    $("#interesting-factors-for-day-chart-modal").modal();
                }
            }).fail((jqxhr, textStatus, error) => {
                $.growl.error({message: error});
            }).always(() => {
                $.unblockUI();
            });

            return false;
        });

        $("[data-fraction-digits]").on("input", e => {
            const $input = $(e.target);
            const asNum = parseFloat($input.val());
            const numFractionDigits = $input.data("fraction-digits");
            if (asNum && numFractionDigits) {
                $input.val(asNum.toFixed(numFractionDigits))
            }
        }).trigger("input");

        const $exactTweetPopover = $("#exact-tweet-popover");
        $exactTweetPopover.popover();
        $exactTweetPopover.on('show.bs.popover', function () {
            $.getJSON(`/anagrams/exacttweetcount`).done(data => {
                if (data.count) {
                    const totalMatches = Number({{stats.allTimeCounts.total_count}});
                    const exactTweetCount = Number(data.count);
                    const formattedExactTweetCount = exactTweetCount.toLocaleString();
                    $("#exact-tweet-number").text(formattedExactTweetCount);
                    $("#approximately").text("exactly");
                    $("#exact-number-of-tweets").text(formattedExactTweetCount);

                    const updatedAllTimeTweetsPerMatch = exactTweetCount / totalMatches;
                    const formattedUpdatedAllTimeTweetsPerMatch = Math.round(updatedAllTimeTweetsPerMatch).toLocaleString();
                    $("#all-time-tweets-per-match").text(formattedUpdatedAllTimeTweetsPerMatch);
                } else if (data.error) {
                    $.growl.error({message: data.error});
                } else {
                    $.growl.error({message: "something went wrong"});
                }
            }).fail((jqxhr, textStatus, error) => {
                $.growl.error({message: error});
            });
        });

        const $exactTweetForPastNumberOfDaysPopover = $("#exact-tweet-for-past-number-of-days-popover");
        $exactTweetForPastNumberOfDaysPopover.popover();
        $exactTweetForPastNumberOfDaysPopover.on('show.bs.popover', function () {
            $.getJSON(`/anagrams/exacttweetcountforpastnumberofdays/{{stats.numberOfPastDays}}`).done(data => {
                if (data.count) {
                    const exactRecentTweetCount = Number(data.count);
                    const formattedExactRecentTweetCount = Number(data.count).toLocaleString();
                    $("#exact-tweet-number-for-past-number-of-days").text(formattedExactRecentTweetCount);
                    $("#exact-tweet-for-past-number-of-days-content").text(formattedExactRecentTweetCount);
                    const recentMatchCount = Number({{stats.recentCounts.recent_total_count}});
                    const recentTweetsPerMatch = exactRecentTweetCount / recentMatchCount;
                    const formattedRecentTweetsPerMatch = Math.round(recentTweetsPerMatch).toLocaleString()
                    $("#recent-tweets-per-match").html(`, for <strong>${formattedRecentTweetsPerMatch}</strong> tweets per match.`);
                } else if (data.error) {
                    $.growl.error({message: data.error});
                } else {
                    $.growl.error({message: "something went wrong"});
                }
            }).fail((jqxhr, textStatus, error) => {
                $.growl.error({message: error});
            });
        });

        function getDateOfOldestExistenceCheckedTweet() {
            $.getJSON(`/anagrams/dateofoldesttweetwhoseexistencehasnotbeenchecked`).done(data => {
                console.log(`polled /anagrams/dateofoldesttweetwhoseexistencehasnotbeenchecked, got ${JSON.stringify(data)}`);
                if (data.date) {
                    const formattedOldestDateExistenceNotChecked = new Date(data.date).toLocaleString();
                    $("#oldest-existence-not-checked").text(formattedOldestDateExistenceNotChecked);
                    const $oldestChecked = $("#oldest-existence-not-checked-content");
                    $oldestChecked.text(formattedOldestDateExistenceNotChecked);
                    flash($oldestChecked);
                    pollDateOfOldestExistenceCheckedTweet();
                } else if (data.error) {
                    $.growl.error({message: data.error});
                } else {
                    $.growl.error({message: "something went wrong"});
                }
            }).fail((jqxhr, textStatus, error) => {
                $.growl.error({message: error});
            });
        }

        function pollDateOfOldestExistenceCheckedTweet() {
            setTimeout(() => {
                getDateOfOldestExistenceCheckedTweet();
            }, 1000 * 60);
        }

        getDateOfOldestExistenceCheckedTweet();

        function getDateLastMatchCreated() {
            $.getJSON(`/anagrams/dateoflastmatchcreated`).done(data => {
                console.log(`polled /anagrams/dateoflastmatchcreated, got ${JSON.stringify(data)}`);
                if (data.date) {
                    const dateLastMatchCreated = new Date(data.date);
                    renderDateLastMatchCreated(dateLastMatchCreated);
                } else if (data.error) {
                    $.growl.error({message: data.error});
                } else {
                    $.growl.error({message: "something went wrong"});
                }
                pollDateLastMatchCreated();
            }).fail((jqxhr, textStatus, error) => {
                $.growl.error({message: error});
            });
        }

        function pollDateLastMatchCreated() {
            setTimeout(() => {
                getDateLastMatchCreated();
            }, 15000);
        }

        function renderDateLastMatchCreated(date) {
            const $template = $("#date-most-recent-match-created-template");
            const $target = $("#date-most-recent-match-created-content");
            const template = Handlebars.compile($template.html());
            const html = template({dateLastMatchCreated: date});
            $target.html(html);
            flash($target);
        }
        const dateLastMatchCreatedFromServer = new Date("{{stats.dateLastMatchCreated}}");
        renderDateLastMatchCreated(dateLastMatchCreatedFromServer);
        pollDateLastMatchCreated();

        $(".js-color-based-on-number-value").each((index, element) => {
            $el = $(element);
            const numberValue = Number($el.text());
            if (numberValue > 0) {
                $el.addClass("success");
            } else if (numberValue < 0) {
                $el.addClass("danger");
            }
        });

        function flash($el) {
            $el.addClass("flash");
            setTimeout(() => {
                $el.removeClass("flash");
            }, 5000)
        }
    });

</script>
<style type="text/css">
    .tableFloatingHeaderOriginal th {
        background-color: #fff;
        border-bottom: 1px solid #DDD;
    }

    .popover-content {
        text-align: center
    }
</style>
